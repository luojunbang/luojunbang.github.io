{
  "name": "luojunbang.github.io",
  "version": "0.1.0",
  "packageManager": "pnpm@7.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "vue": "pnpm run -C ./apps/web-vue3 serve",
    "react": "pnpm run -C ./apps/web-react serve",
    "build:web": "pnpm run --filter @lo/web-* build",
    "place": "mv ./apps/main/out ./dist && mv ./apps/web-vue3/dist ./dist/vue-page && mv ./apps/web-react/build ./dist/react-page"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "lo-utils": "^1.2.27"
  },
  "devDependencies": {
    "chalk": "^4.1.2",
    "prettier": "^3.0.1",
    "serve": "^14.2.0",
    "webpack-cli": "^5.1.4"
  }
}
