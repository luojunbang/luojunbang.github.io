{
  "name": "luojunbang.github.io",
  "version": "0.1.0",
  "packageManager": "pnpm@7.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "vue": "pnpm run -C ./apps/web-vue3 serve",
    "react": "pnpm run -C ./apps/web-react serve",
    "build:vue": "rimraf ./dist &&  pnpm run -C ./apps/vue3-web build",
    "build:gh-page": "rimraf ./dist &&  pnpm run -C ./packages/vue-components build &&  pnpm run -C ./apps/vue3-web build",
    "webpack:build": "webpack --config  --mode=producticon ./webpack.config.js",
    "vite": "vite",
    "vite:build": "vite build",
    "vite:review": "vite preview"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "lo-utils": "^1.2.27"
  },
  "devDependencies": {
    "chalk": "^5.3.0",
    "connect": "^3.7.0",
    "prettier": "^3.0.1",
    "serve-static": "^1.15.0"
  }
}
