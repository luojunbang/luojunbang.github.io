{
  "compilerOptions": {
    "noImplicitAny": false,
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    // "baseUrl": "./",
    "types": [
      // "vite/client"
      "webpack-env",
      "element-plus/global"
    ],
    "paths": {
      /* 指定一组将导入重新映射到其他查找位置的条目。 */
      // "@/*": ["src/*"],
      "@lo-vue3/*": ["packages/*"]
      // "@/*": ["vue3-web/src/*"],
      // "@react/*": ["react-web/*"]
    },
    "lib": ["esnext", "dom", "dom.iterable", "scripthost"]
  },
  "include": ["vue3-web/*"],
  "exclude": ["node_modules"]
}
