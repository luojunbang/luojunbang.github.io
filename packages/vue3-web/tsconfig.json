{
  "include": ["src/**/*.ts", "src/**/*.vue"], //只编译的文件
  "exclude": ["node_modules"], //排除之外不编译的文件
  "compilerOptions": {
    "types": ["webpack-env", "element-plus/global"],
    "paths": {
      /* 指定一组将导入重新映射到其他查找位置的条目。 */
      "@/*": ["./src/*"]
    }
  },
  "references": [{ "path": "../../tsconfig.vue.json" }]
}
