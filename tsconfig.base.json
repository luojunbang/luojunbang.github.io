{
    "compilerOptions": {
      "target": "es2018",
      "module": "esnext",
      "sourceMap": false,
      "moduleResolution": "node",
      "allowJs": false,
      "strict": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "removeComments": false,
      "noUnusedLocals":false,
      "noUnusedParameters":false,
      "rootDir": ".",
      "types": [],
      "paths": {
        "@lo/*": ["packages/*"]
      }
    }
  }
  